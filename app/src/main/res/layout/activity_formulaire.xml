        android:textColor="#000000"
        android:textSize="18sp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="483dp"
        tools:layout_editor_absoluteY="445dp" />

    <TextView
        android:id="@+id/Ville"
        android:layout_width="158dp"
        android:layout_height="29dp"
        android:text="Ville:"
        android:textColor="#000000"
        android:textSize="18sp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="3dp"
        tools:layout_editor_absoluteY="537dp" />

    <TextView
        android:id="@+id/Tel"
        android:layout_width="182dp"
        android:layout_height="24dp"
        android:text="Numéro de téléphone :"
        android:textColor="#000000"
        android:textSize="18sp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="22dp"
        tools:layout_editor_absoluteY="595dp" />

    <TextView
        android:id="@+id/Email"
        android:layout_width="74dp"
        android:layout_height="25dp"
        android:text="E-mail:"
        android:textColor="#000000"
        android:textSize="18sp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="483dp"
        tools:layout_editor_absoluteY="512dp" />

    <EditText
        android:id="@+id/editTextNomPAP"
        android:layout_width="127dp"
        android:layout_height="30dp"
        android:layout_marginTop="156dp"
        android:ems="10"
        android:inputType="date"
        app:layout_constraintTop_toBottomOf="@+id/textViewPAP"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="188dp" />

    <EditText
        android:id="@+id/editTextPrenomPAP"
        android:layout_width="193dp"
        android:layout_height="31dp"
        android:ems="10"
        android:inputType="date"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="122dp"
        tools:layout_editor_absoluteY="459dp" />

    <EditText
        android:id="@+id/editTextAdresse"
        android:layout_width="193dp"
        android:layout_height="31dp"
        android:ems="10"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="213dp"
        tools:layout_editor_absoluteY="490dp" />

    <EditText
        android:id="@+id/editText6"
        android:layout_width="118dp"
        android:layout_height="36dp"
        android:ems="10"
        android:inputType="numberDecimal"
        tools:layout_editor_absoluteX="618dp"
        tools:layout_editor_absoluteY="443dp"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/editTextVille"
        android:layout_width="193dp"
        android:layout_height="31dp"
        android:ems="10"
